@using InterfaceWEBHealthcare.Models.Treatments;
@model List<Medicine>;
@{
    ViewData["Title"] = "Privacy Policy";
}

<table class="table table-bordered table-dark" id="tblDataTable">
    <thead class="thead-light">
        <tr>
            <th scope="col">startDate</th>
            <th scope="col">endDate</th>
            <th scope="col">instructions</th>
            <th scope="col">instructionsNotes</th>
            <th scope="col">status</th>
            <th scope="col">dosage</th>
            <th scope="col">quantity</th>
            <th scope="col">route</th>
            <th scope="col">form</th>
            <th scope="col">frequency</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr id="trData">
            <td><span class="pd-AppoinmentDate">@item.StartDate</span></td>
            <td><span class="pd-AppoinmentsStatus">@item.EndDate</span></td>
            <td><span class="pd-AppoinmentDate">@item.Instructions</span></td>
            <td><span class="pd-AppoinmentsStatus">@item.InstructionsNotes</span></td>
            <td><span class="pd-AppoinmentDate">@item.Status</span></td>
            <td><span class="pd-AppoinmentsStatus">@item.Dosage</span></td>
            <td><span class="pd-AppoinmentDate">@item.Quantity</span></td>
            <td><span class="pd-AppoinmentsStatus">@item.Route</span></td>
            <td><span class="pd-AppoinmentDate">@item.Form</span></td>
            <td><span class="pd-AppoinmentsStatus">@item.Frequency</span></td>
        </tr>
        }
    </tbody>
</table>

